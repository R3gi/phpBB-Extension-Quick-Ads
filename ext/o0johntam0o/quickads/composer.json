{
	"name": "o0johntam0o/quickads",
	"type": "phpbb-extension",
	"description": "Append any Ads, HTML, jQuery/JS code into your forum quickly.",
	"homepage": "https://github.com/o0johntam0o/phpBB-Extension-Quick-Ads",
	"version": "2.0.0",
	"keywords": [
		"o0johntam0o",
		"extension",
		"quick",
		"ads"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Huynh Buu Tam",
			"email": "o0johntam0o@gmail.com",
			"homepage": "https://github.com/o0johntam0o",
			"role": "Lead Developer"
		},
		{
			"name": "Contributors",
			"homepage": "https://github.com/o0johntam0o/phpBB-Extension-Quick-Ads/graphs/contributors"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Quick Ads",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/quickads",
			"filename": "version_check"
		}
	}
}
